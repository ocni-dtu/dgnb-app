  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8000 --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "format:fix": "prettier --write src/",
    "format": "prettier --check src/",
    "lint": "eslint src --ext ts,tsx ",
